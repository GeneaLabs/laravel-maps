{
  "name": "genealabs/laravel-maps",
  "description": "Eay - peasy map integration for Laravel.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpgmaps",
    "googlemaps",
    "biostall"
  ],
  "authors": [
    {
      "name": "Mike Bronner",
      "email": "mike@genealabs.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^5.5"
  },
  "suggest": {
    "geocoder-php/GeocoderLaravel": "If you need to geocode addresses and don't already have marker coordinates, use this package. It provides geocoding functionality optimized for Laravel."
  },
  "autoload": {
    "psr-4": {
      "GeneaLabs\\LaravelMaps\\": "src/"
    }
  },
  "require-dev": {
      "codedungeon/phpunit-result-printer": "*",
      "fzaninotto/faker": "*",
      "mockery/mockery": "*",
      "orchestra/database": "3.8.x-dev@dev",
      "orchestra/testbench-browser-kit": "3.8.*",
      "orchestra/testbench-dusk": "3.8.*",
      "orchestra/testbench": "3.8.*",
      "php-coveralls/php-coveralls" : "*",
      "phpmd/phpmd": "*",
      "phpunit/phpunit": "*",
      "sebastian/phpcpd": "*",
      "symfony/thanks": "*"
  },
  "autoload-dev": {
    "psr-4": {
        "GeneaLabs\\LaravelMaps\\Tests\\": "tests/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "GeneaLabs\\LaravelMaps\\Providers\\Service"
          ],
          "aliases": {
              "Map": "GeneaLabs\\LaravelMaps\\Facades\\Map"
          }
      }
  },
  "minimum-stability": "stable"
}
